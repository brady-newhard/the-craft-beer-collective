<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>My Breweries</title>
    <link rel="stylesheet" href="/css/styles.css">
  </head>
  <body>
    <%- include('../partials/_navbar.ejs') %>
    <h1>My Breweries</h1>
    <a href="/users/<%=user._id%>/breweries/new"><button>Add a Brewery</button></a>
    <ul>
        <% breweries.forEach((brewery)=>{ %>
        <li><%= brewery.name %>
            <!-- Location: <%= brewery.location %>
            Brewery Link: <%= brewery.link %>
            Beer Consumed: <%= brewery.beer %>
            Brewery Rating: <%= brewery.rating %>
            Comments: <%= brewery.comment %> -->
        </li>
        <% }) %>
      </ul>
  </body>
  
</html>



